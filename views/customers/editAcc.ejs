<h2>Edit Account</h2>
<form action="/customers/<%= customer.id %>/<%= account.id %>?_method=PUT" method="POST">
    <%- include('_form_account.ejs') %> 
    <p>Customer name: <%=customer.first_name + " " + customer.last_name %></p>
    <p>Account number: <%=account.account_number%></p>
    <a class="btn btn-primary" href="/customers/<%= customer.id %>">Cancel</a>
    <button class="btn btn-primary" type="submit">Update</button>
</form>